  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Test",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "test"
      ],
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outputCapture": "std",
      "env": {
        "LOG_LEVEL": "verbose"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "xlsx encoding",
      "program": "${workspaceFolder}/test/xlsx/encoding.js",
      "outputCapture": "std",
      "env": {
        "LOG_LEVEL": "debug"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "xlsx transfers",
      "program": "${workspaceFolder}/test/xlsx/transfers.js",
      "outputCapture": "std",
      "env": {
        "LOG_LEVEL": "verbose"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "xlsx store",
      "program": "${workspaceFolder}/test/xlsx/store.js",
      "outputCapture": "std",
      "env": {
        "LOG_LEVEL": "verbose"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "xlsx recall",
      "program": "${workspaceFolder}/test/xlsx/recall.js",
      "outputCapture": "std",
      "env": {
        "LOG_LEVEL": "verbose"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "xlsx retrieve",
      "program": "${workspaceFolder}/test/xlsx/retrieve.js",
      "outputCapture": "std",
      "env": {
        "LOG_LEVEL": "verbose"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "xlsx dull",
      "program": "${workspaceFolder}/test/xlsx/dull.js",
      "outputCapture": "std",
      "env": {
        "LOG_LEVEL": "verbose"
      }
    }
  ]
}
