  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Run Test",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "test"],
      "skipFiles": ["<node_internals>/**"],
      "outputCapture": "std",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "verbose"
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "xlsx encoding",
      "program": "${workspaceFolder}/test/xlsx/encoding.js",
      "outputCapture": "std",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "debug"
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "xlsx transfers",
      "program": "${workspaceFolder}/test/xlsx/transfers.js",
      "outputCapture": "std",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "verbose"
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "xlsx store",
      "program": "${workspaceFolder}/test/xlsx/store.js",
      "outputCapture": "std",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "verbose"
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "xlsx recall",
      "program": "${workspaceFolder}/test/xlsx/recall.js",
      "outputCapture": "std",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "verbose"
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "xlsx retrieve",
      "program": "${workspaceFolder}/test/xlsx/retrieve.js",
      "outputCapture": "std",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "verbose"
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "xlsx dull",
      "program": "${workspaceFolder}/test/xlsx/dull.js",
      "outputCapture": "std",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "verbose"
      }
    }
  ]
}
